@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="@Model.Value("pageDescription")"/>
    <meta name="keywords" content="@Model.Value("pageKeywords")">
    <title>@Model.Value("pageTitle") | Crito - Agency Service</title>
    <script
      src="https://kit.fontawesome.com/41c8b474e2.js"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />
    
    <link rel="stylesheet" href='@Url.Content("~/css/style.css")' />
  </head>
  <body>
      
      <div class="wrapper">
          
        @await Html.PartialAsync("_Navbar")
        @await Html.PartialAsync("_Breadcrumb")
            
        <main>
            @RenderBody()
        </main>
          
        @await Html.PartialAsync("_Footer")
        
      </div>

      


    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>
    <script src="~/scripts/site.js"></script>
  </body>
</html>